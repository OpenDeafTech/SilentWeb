#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/bin/node_modules:/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/node_modules:/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules:/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/bin/node_modules:/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/node_modules:/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules:/home/vincentdev/Documents/GitHub/SilentWeb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../clean-css-cli/bin/cleancss" "$@"
else
  exec node  "$basedir/../clean-css-cli/bin/cleancss" "$@"
fi
